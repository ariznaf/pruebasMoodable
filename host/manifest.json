{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
		"$(MODDABLE)/examples/manifest_net.json",
		"$(MODDABLE)/modules/pins/pwm/manifest.json"
	],
	"creation": {
		"keys": {
			"available": 96
		}
	},
	"defines": {
		"XS_MODS": 1
	},
	"modules": {
		"*": [
			"./main",
			"./loadmod/*",
			"$(MODULES)/network/websocket/*",
			"$(MODULES)/data/base64/*",
			"$(MODULES)/crypt/digest/*",
			"$(MODULES)/crypt/digest/kcl/*",
			"$(MODULES)/data/logical/*",
			"../../moddable/modules/base/console/*",
			"../../moddable/modules/base/cli/*",
			"$(MODULES)/network/mqtt/*"
		]
	},
	"preload": [
		"websocket",
		"console",
		"cli",
		"base64",
		"logical",
		"mqtt"
	],
	"strip": [
		"Atomics",
		"eval",
		"Function",
		"Generator",
		"Map",
		"Proxy",
		"Reflect",
		"RegExp",
		"Set",
		"SharedArrayBuffer",
		"WeakMap",
		"WeakSet"
	],
	"build": {
		"NAME": "pru"
	},
	"platforms": {
		"esp32": {
			"modules": {
				"~": [
					"$(BUILD)/devices/esp32/setup/network"
				]

			}
		}
	}
}
