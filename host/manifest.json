{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
        "$(MODDABLE)/examples/manifest_net.json",
        "$(MODDABLE)/modules/pins/pwm/manifest.json",
		"$(MODDABLE)/modules/network/mqtt/manifest.json",
		"$(MODDABLE)/modules/pins/i2c/manifest.json"
	],
	"creation": {
		"keys": {
			"available": 96
		}
	},
	"defines": {
		"XS_MODS": 1
	},
	"modules": {
		"*": [
			"./main",
			"./loadmod/*",
			"$(MODULES)/network/websocket/*",
			"$(MODULES)/network/mqtt/*"
		],
		"pins/smbus": "$(MODDABLE)/modules/pins/smbus/smbus"

	},
	"preload": [
		"websocket",
		"mqtt",
		"pins/i2c",
		"pins/smbus"
	],
	"strip": [
		"Atomics",
		"eval",
		"Function",
		"Generator",
		"Map",
		"Proxy",
		"Reflect",
		"RegExp",
		"Set",
		"SharedArrayBuffer",
		"WeakMap",
		"WeakSet"
	],
	"build": {
		"NAME": "host"
	},
	"platforms": {
		"esp32": {
			"modules": {
				"~": [
					"$(BUILD)/devices/esp32/setup/network"
				]

			},
			"defines": {
                "i2c": {
                    "sda_pin": 21,
                    "scl_pin": 22
                }
            }

		}
	}

}
